<Project>
    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <VersionPrefix>0.4.9</VersionPrefix>
        <!--<VersionSuffix>preview2</VersionSuffix>-->
        <VersionSuffix Condition=" '$(VersionSuffix)' != '' AND '$(BuildNumber)' != '' ">
            $(VersionSuffix)-$(BuildNumber)
        </VersionSuffix>
    </PropertyGroup>

    <PropertyGroup>
        <Authors>Mo3in</Authors>
        <CodeAnalysisSummary>some tools for fast developing back-end dotnet</CodeAnalysisSummary>
        <Description>some tools for fast developing back-end.</Description>
        <!--<PackageIconUrl>https://s3.amazonaws.com/automapper/icon.png</PackageIconUrl>-->
        <PackageProjectUrl>https://github.com/mo3in/Kasp</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/mo3in/Kasp/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/mo3in/Kasp</RepositoryUrl>
        <LangVersion>latest</LangVersion>
        <PackageTags>Kasp,asp.net core,mvc,framework,deadline</PackageTags>
    </PropertyGroup>
</Project>
